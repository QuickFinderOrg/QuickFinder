@page
@model MatchingModel
@{
    ViewData["Title"] = "Matchmaking";
}

<div class="text-center">
    <h1 class="display-4">Matchmaking</h1>
    <p>Courses</p>
    <ul>
        @foreach (var Course in Model.Courses)
        {
            <li>@Course.Name</li>
        }
    </ul>
    <form method="post">
        <button type="">Start looking</button>
    </form>
    <a href="/Student/Groups">Go To groups</a>
</div>
